Description: Make NSS include path less ambiguous (we want the one provided by libnss3-dev in /usr/include/nss/, not the one installed by libc6-dev in /usr/include/).
Author: Olivier Tilloy <olivier.tilloy@canonical.com>

--- a/xmlsecurity/source/xmlsec/nss/nssinitializer.cxx
+++ b/xmlsecurity/source/xmlsec/nss/nssinitializer.cxx
@@ -43,7 +43,7 @@
 
 #include <nspr.h>
 #include <cert.h>
-#include <nss.h>
+#include <nss/nss.h>
 #include <pk11pub.h>
 #include <secmod.h>
 #include <nssckbi.h>
--- a/onlineupdate/source/update/updater/updater.cxx
+++ b/onlineupdate/source/update/updater/updater.cxx
@@ -114,7 +114,7 @@ struct UpdateServerThreadArgs
 #endif
 
 #if defined(VERIFY_MAR_SIGNATURE) && !defined(_WIN32) && !defined(MACOSX)
-#include <nss.h>
+#include <nss/nss.h>
 #include <nspr.h>
 #endif
 
--- a/comphelper/source/misc/hash.cxx
+++ b/comphelper/source/misc/hash.cxx
@@ -14,7 +14,7 @@
 #include <config_oox.h>
 
 #if USE_TLS_NSS
-#include <nss.h>
+#include <nss/nss.h>
 #include <pk11pub.h>
 #include <sechash.h>
 #elif USE_TLS_OPENSSL
