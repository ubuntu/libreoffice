+
+<xs:group name="lowlimit.content">
+  <xs:sequence>
+    <xs:group ref="Content-expr.class"/>
+  </xs:sequence>
+</xs:group>
+
+<xs:complexType name="lowlimit.type">
+  <xs:group ref="lowlimit.content"  minOccurs="1" maxOccurs="unbounded"/>
+  <xs:attributeGroup ref="lowlimit.attlist"/>
