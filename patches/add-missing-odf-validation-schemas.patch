+
+<xs:group name="Content-functions.class">
+  <xs:choice>
+    <xs:element ref="compose"/>
+    <xs:element ref="domain"/>
+    <xs:element ref="codomain"/>
+    <xs:element ref="image"/>
+    <xs:element ref="domainofapplication"/>
+    <xs:element ref="ident"/>
