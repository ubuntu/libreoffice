+    <data type="duration"/>
+</define>
+<define name="integer">
+    <data type="integer"/>
+</define>
+<define name="nonNegativeInteger">
+    <data type="nonNegativeInteger"/>
+</define>
+<define name="positiveInteger">
+    <data type="positiveInteger"/>
+</define>
+<define name="double">
+    <data type="double"/>
+</define>
+<define name="anyURI">
+    <data type="anyURI"/>
+</define>
