+
+<xs:complexType name="ci.type" mixed="true">
+  <xs:group ref="Content-token.content" minOccurs="0"/>
+  <xs:attributeGroup ref="ci.attlist"/>
+</xs:complexType>
+
+<xs:element name="ci" type="ci.type"/>
+
